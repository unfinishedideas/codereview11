<%= render "layouts/errors", :object => @product %>
<hr>
<%= form_for @product do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <br>
  <%= f.label :cost %>
  <%= f.text_field :cost %>
  <br>
  <%= f.label :country_of_origin %>
  <%= f.text_field :country_of_origin %>
  <hr>
  <%= f.submit  %>
<% end %>
